:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x55ca279efc50 .scope module, "tb_half_subtractor" "tb_half_subtractor" 2 1;
 .timescale 0 0;
v0x55ca27a02010_0 .var "a", 0 0;
v0x55ca27a020d0_0 .var "b", 0 0;
v0x55ca27a021a0_0 .net "borrow", 0 0, v0x55ca27a01dd0_0;  1 drivers
v0x55ca27a022a0_0 .net "diff", 0 0, v0x55ca27a01ea0_0;  1 drivers
S_0x55ca279efde0 .scope module, "uut" "half_subtractor_behavioral" 2 5, 3 1 0, S_0x55ca279efc50;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "a";
    .port_info 1 /INPUT 1 "b";
    .port_info 2 /OUTPUT 1 "diff";
    .port_info 3 /OUTPUT 1 "borrow";
v0x55ca279f0000_0 .net "a", 0 0, v0x55ca27a02010_0;  1 drivers
v0x55ca27a01d10_0 .net "b", 0 0, v0x55ca27a020d0_0;  1 drivers
v0x55ca27a01dd0_0 .var "borrow", 0 0;
v0x55ca27a01ea0_0 .var "diff", 0 0;
E_0x55ca279b6de0 .event anyedge, v0x55ca27a01d10_0, v0x55ca279f0000_0;
    .scope S_0x55ca279efde0;
T_0 ;
    %wait E_0x55ca279b6de0;
    %load/vec4 v0x55ca279f0000_0;
    %load/vec4 v0x55ca27a01d10_0;
    %xor;
    %store/vec4 v0x55ca27a01ea0_0, 0, 1;
    %load/vec4 v0x55ca279f0000_0;
    %inv;
    %load/vec4 v0x55ca27a01d10_0;
    %and;
    %store/vec4 v0x55ca27a01dd0_0, 0, 1;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x55ca279efc50;
T_1 ;
    %vpi_call 2 13 "$dumpfile", "half_sub_behavioral.vcd" {0 0 0};
    %vpi_call 2 14 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x55ca279efc50 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55ca27a02010_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55ca27a020d0_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55ca27a02010_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55ca27a020d0_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55ca27a02010_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55ca27a020d0_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55ca27a02010_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55ca27a020d0_0, 0, 1;
    %delay 10, 0;
    %vpi_call 2 21 "$finish" {0 0 0};
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
