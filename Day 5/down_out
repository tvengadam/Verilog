#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x55586f025ab0 .scope module, "down_counter_tb" "down_counter_tb" 2 1;
 .timescale 0 0;
v0x55586f037fb0_0 .var "clk", 0 0;
v0x55586f038080_0 .net "count", 3 0, v0x55586f037db0_0;  1 drivers
v0x55586f038150_0 .var "rst", 0 0;
S_0x55586f025c40 .scope module, "uut" "down_counter" 2 8, 3 1 0, S_0x55586f025ab0;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "rst";
    .port_info 2 /OUTPUT 4 "count";
v0x55586efec490_0 .net "clk", 0 0, v0x55586f037fb0_0;  1 drivers
v0x55586f037db0_0 .var "count", 3 0;
v0x55586f037e90_0 .net "rst", 0 0, v0x55586f038150_0;  1 drivers
E_0x55586efed6a0 .event posedge, v0x55586efec490_0;
    .scope S_0x55586f025c40;
T_0 ;
    %wait E_0x55586efed6a0;
    %load/vec4 v0x55586f037e90_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 15, 0, 4;
    %assign/vec4 v0x55586f037db0_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x55586f037db0_0;
    %subi 1, 0, 4;
    %assign/vec4 v0x55586f037db0_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x55586f025ab0;
T_1 ;
    %delay 10, 0;
    %load/vec4 v0x55586f037fb0_0;
    %inv;
    %store/vec4 v0x55586f037fb0_0, 0, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_0x55586f025ab0;
T_2 ;
    %vpi_call 2 18 "$monitor", "Time = %0t | rst = %b | clk = %b | count = %b", $time, v0x55586f038150_0, v0x55586f037fb0_0, v0x55586f038080_0 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55586f037fb0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55586f038150_0, 0, 1;
    %delay 15, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55586f038150_0, 0, 1;
    %delay 100, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55586f038150_0, 0, 1;
    %delay 20, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55586f038150_0, 0, 1;
    %delay 100, 0;
    %vpi_call 2 28 "$finish" {0 0 0};
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "down_countertb.v";
    "down_counter.v";
