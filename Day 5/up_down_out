#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x5570f4bb7070 .scope module, "up_down_counter_tb" "up_down_counter_tb" 2 1;
 .timescale 0 0;
v0x5570f4bca0e0_0 .var "clk", 0 0;
v0x5570f4bca1a0_0 .net "count", 3 0, v0x5570f4bc9dc0_0;  1 drivers
v0x5570f4bca270_0 .var "rst", 0 0;
v0x5570f4bca370_0 .var "up_down", 0 0;
S_0x5570f4bb7200 .scope module, "uut" "up_down_counter" 2 7, 3 1 0, S_0x5570f4bb7070;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "rst";
    .port_info 2 /INPUT 1 "up_down";
    .port_info 3 /OUTPUT 4 "count";
v0x5570f4b7d490_0 .net "clk", 0 0, v0x5570f4bca0e0_0;  1 drivers
v0x5570f4bc9dc0_0 .var "count", 3 0;
v0x5570f4bc9ea0_0 .net "rst", 0 0, v0x5570f4bca270_0;  1 drivers
v0x5570f4bc9f70_0 .net "up_down", 0 0, v0x5570f4bca370_0;  1 drivers
E_0x5570f4bb62e0 .event posedge, v0x5570f4b7d490_0;
    .scope S_0x5570f4bb7200;
T_0 ;
    %wait E_0x5570f4bb62e0;
    %load/vec4 v0x5570f4bc9ea0_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v0x5570f4bc9dc0_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x5570f4bc9f70_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.2, 8;
    %load/vec4 v0x5570f4bc9dc0_0;
    %addi 1, 0, 4;
    %assign/vec4 v0x5570f4bc9dc0_0, 0;
    %jmp T_0.3;
T_0.2 ;
    %load/vec4 v0x5570f4bc9dc0_0;
    %subi 1, 0, 4;
    %assign/vec4 v0x5570f4bc9dc0_0, 0;
T_0.3 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x5570f4bb7070;
T_1 ;
    %delay 10, 0;
    %load/vec4 v0x5570f4bca0e0_0;
    %inv;
    %store/vec4 v0x5570f4bca0e0_0, 0, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_0x5570f4bb7070;
T_2 ;
    %vpi_call 2 18 "$monitor", "time = %0t | clk = %b | rst = %b | up_down = %b", $time, v0x5570f4bca0e0_0, v0x5570f4bca270_0, v0x5570f4bca370_0 {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5570f4bca0e0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5570f4bca270_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5570f4bca370_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5570f4bca270_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5570f4bca370_0, 0, 1;
    %delay 20, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5570f4bca270_0, 0, 1;
    %vpi_call 2 25 "$finish" {0 0 0};
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "up_down_counter_tb.v";
